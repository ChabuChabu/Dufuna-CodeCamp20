{
    "require": {
        "slim/slim": "4.*",
        "slim/psr7": "^1.3",
        "vlucas/phpdotenv": "^5.3",
        "illuminate/database": "^8.36"
    },
    "autoload": {
        "psr-4": {
            "App\\Controller\\": "app/controllers",
            "App\\Middleware\\": "app/middlewares",
            "App\\Model\\": "app/models"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t bootstrap bootstrap/app.php"
    },
    "config": {
        "process-timeout": 0
    }
}
