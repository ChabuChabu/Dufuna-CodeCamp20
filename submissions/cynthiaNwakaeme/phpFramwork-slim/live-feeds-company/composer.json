{   "name": "dufuna/live-feeds-company",
    "description": "A simple Slim Application to generate API for publishing articles",
    "require": {
        "slim/slim": "4.*",
        "slim/psr7": "^1.3",
        "vlucas/phpdotenv": "^5.3",
        "illuminate/database": "^8.44"
    },
    "autoload": {
        "psr-4": {
            "DB\\":"database",
            "App\\Controller\\": "app/controllers",
            "App\\Model\\": "app/models"
        }
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public"
    },
    "config": {
        "process-timeout": 0
    }
}
